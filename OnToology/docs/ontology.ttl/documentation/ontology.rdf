<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#"
     xml:base="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:pec2_rc_wine_ontology="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology">
        <terms:license>http://purl.org/NET/rdflicense/cc-by4.0</terms:license>
        <terms:title>Wine Ontology PEC2</terms:title>
        <rdfs:comment>Wine Ontology for Recommendation System
Authors: Yago Estévez Davila, José David Zapata García</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasAging -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasAging">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Aging"/>
        <rdfs:comment>Provides information about the wine aging category</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasBody -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasBody">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasCharacteristics"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#WineBody"/>
        <rdfs:comment>The wine body can be light, medium or full</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasCharacteristics -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasCharacteristics">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#WineCharasteristics"/>
        <rdfs:comment>Provides information about the wine&apos;s organoleptic properties</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasColour -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasColour">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasCharacteristics"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#WineColour"/>
        <rdfs:comment>The wine colour can be red, rose or white</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasFlavour -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasFlavour">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasCharacteristics"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#WineFlavour"/>
        <rdfs:comment>The wine flavour can be delicate, moderate or strong</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasFood -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasFood">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#usedToPrepare"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Dishes"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Edibles"/>
        <rdfs:comment>Provides information about the ingredients used for each course</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasMaker -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasMaker">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#makesWine"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Winery"/>
        <rdfs:comment>Associates each wine with its correspondering winery</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasPairing -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasPairing">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Dishes"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:comment>Provides recommendations for food and wine pairing</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasRegion -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasRegion">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Region"/>
        <rdfs:comment>Provides the region where the wine was produced</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasSugar -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasSugar">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasCharacteristics"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#WineSugar"/>
        <rdfs:comment>Wine&apos;s sweetnes, can be dry, offdry or sweet</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasType -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasType">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#WineType"/>
        <rdfs:comment>Used to associated each wine with its corresponding type</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasWine -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasWine">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#WineType"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:comment>Provides the wines made by each winery</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasWinery -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasWinery">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#locatedIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Region"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Winery"/>
        <rdfs:comment>Associates each winery with its region</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#locatedIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#locatedIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Winery"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Region"/>
        <rdfs:comment>Inverse of hasWinery</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#madeIntoWine -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#madeIntoWine">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#madeWithGrape"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Grape"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:comment>Provides information about the wines made from a given grape</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#madeWithGrape -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#madeWithGrape">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Grape"/>
        <rdfs:comment>Provides information about the grape(s) used to make a given wine</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#makesWine -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#makesWine">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Winery"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:comment>Provides the list of wines made by a given winery</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#usedToPrepare -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#usedToPrepare">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Edibles"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Dishes"/>
        <rdfs:comment>Provides information about the courses that can be made with each food</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasConservationTemperature -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasConservationTemperature">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Recommended preservation temperature for the wine as given by its maker</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasDesignation -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasDesignation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Provides information about wine&apos;s origin designation</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasRating -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasRating">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Numerical mean rating between 1.0 and 5.0</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasYears -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasYears">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>Wine&apos;s aging years</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Aging -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Aging">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#WineCharasteristics"/>
        <sameAs>https://dbpedia.org/page/Aging_of_wine</sameAs>
        <rdfs:comment>This class provides information about the wine&apos;s aging</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Consumables -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Consumables">
        <rdfs:comment>Everything that is able for consumption</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Crianza -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Crianza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Aging"/>
        <rdfs:comment>These wines usually have 24 or less months of aging</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Desserts -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Desserts">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Dishes"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#ItalianDishes"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Soups"/>
        <rdfs:comment>Sweet meals</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Dishes -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Dishes">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Consumables"/>
        <rdfs:comment>Different courses made with various ingredients</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Drinkables -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Drinkables">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Consumables"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasMaker"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Winery"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#hasYears"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Drinks and simmilar</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Edibles -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Edibles">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Consumables"/>
        <rdfs:comment>Ingredients used for course meals</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Excellent -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Excellent">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Rating"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Good"/>
        <rdfs:comment>Items with puntuation higher or equal than 4 out of 5</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Fowl -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Fowl">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Edibles"/>
        <rdfs:comment>White meat like chicken, rabbit, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Fruit -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Fruit">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Edibles"/>
        <rdfs:comment>Vegetables with pulp arount the seed(s)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Good -->


    <owl:Class rdf:about="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Good">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/david/ontologies/2023/3/pec2_rc_wine_ontology#Rating"/>
